    let a = A #{ age: 14 };
    a.set_age(15);
    print(int__to_string(a.age));
}

//! TEST class_operator_overloading
extern fun int__to_string(s: int) -> string;

class Vector2 {
    x: int, y: int
    
    fun self.add_Vector2(other: Vector2) -> Vector2 {
        Vector2 #{
            x: self.x + other.x,
            y: self.y + other.y
        };
    }

    fun ::new(x: int, y: int) -> Vector2 {
        Vector2 #{ x: x, y: y };
    }
}

class Vector3 {
    x: int, y: int, z: int

    fun self.add_Vector3(other: Vector3) -> Vector3 {
        Vector3 #{
            x: self.x + other.x,
            y: self.y + other.y,
            z: self.z + other.z
        };
    }

    fun ::new(x: int, y: int, z: int) -> Vector3 {
        Vector3 #{ x: x, y: y, z: z };
    }
}

fun main() {
    let v1 = Vector2::new(10, 10);
    let v2 = Vector2::new(15, 10);
    
    let vr = v1 + v2;
    print(int__to_string(vr.x));
    print(int__to_string(vr.y));

    let v1 = Vector3::new(10, 10, 0);
    let v2 = Vector3::new(15, 10, 0);
    
    let vr = v1 + v2;
    print(int__to_string(vr.x));
    print(int__to_string(vr.y));
    print(int__to_string(vr.z));
}

